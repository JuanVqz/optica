<%= form_with model: producto do |f| %>
  <%= f.text_field :nombre %>
  <br/>
  <%= f.text_field :codigo %>
  <br/>
  <%= f.text_area :descripcion %>
  <br/>
  <%= f.select :tipo, tipos %>
  <br/>
  <%= f.number_field :precio_compra %>
  <br/>
  <%= f.number_field :precio_venta %>
  <br/>
  <%= f.number_field :existencia %>
  <br/>
  <%= f.submit %>
<% end %>
