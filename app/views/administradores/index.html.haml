.block.block-themed
  .block-header.bg-primary-dark
    %h3.block-title
      %i.fa.fa-users
      Lista de administradores
    .block-options
      .block-options-item
        = link_to [:new, :administrador], class: 'btn btn-alt-secondary' do
          %i.fa.fa-plus
          registrar administrador
  .block-content
    = render 'usuarios/table', usuarios: @administradores
